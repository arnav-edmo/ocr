{
  "version": 2,
  "builds": [
    {
      "src": "output/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/viewer.html",
      "dest": "/viewer.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}

